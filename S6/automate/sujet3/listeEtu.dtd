        <!ELEMENT id (#PCDATA)>
        <!ELEMENT nom (#PCDATA)>
        <!ELEMENT prenom (#PCDATA)>
        <!ELEMENT age (#PCDATA)>
        <!ELEMENT moyenne (#PCDATA)>
        <!ELEMENT adresse EMPTY>
